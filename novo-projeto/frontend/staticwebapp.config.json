{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "allowedRoles": ["anonymous"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/api/*", "*.{png,jpg,gif,ico,css,js}"]
    },
    "platform": {
        "apiRuntime": "python:3.11"
    }
  }
